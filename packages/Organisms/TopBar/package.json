{
  "name": "@numl-react/top-bar",
  "version": "0.0.1",
  "description": "A simple and accessible component for React by numl design",
  "keywords": [
    "design",
    "system",
    "components",
    "react",
    "numl",
    "uikit"
  ],
  "author": "Numl.Design Team",
  "license": "MIT",
  "homepage": "https://github.com/numldesign/numl-react#readme",
  "scripts": {
    "postbuild": "yarn build:types",
    "build": "vite build",
    "build:types": "npx tsc index.ts --esModuleInterop --skipLibCheck --skipDefaultLibCheck --declaration --emitDeclarationOnly --outDir dist/types --jsx react"
  },
  "main": "./dist/core.cjs.js",
  "module": "./dist/core.es.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/numl-react.git"
  },
  "bugs": {
    "url": "https://github.com/numldesign/numl-react/issues"
  },
  "dependencies": {
    "@numl-react/core": "^0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
