{
  "name": "@numl-react/elements",
  "version": "0.0.14",
  "description": "Attribute Provider Box for Numl React by NUML.Design.",
  "keywords": [
    "component",
    "elements",
    "initials",
    "react",
    "react-components",
    "profile-image",
    "numl-react",
    "photo"
  ],
  "author": "Numl.Design Team",
  "license": "MIT",
  "homepage": "https://github.com/numldesign/numl-react#readme",
  "scripts": {
    "build": "vite build",
    "postbuild": "npm run build:types",
    "build:types": "tsc --skipLibCheck --esModuleInterop --emitDeclarationOnly --declaration --jsx react --declarationDir dist/types"
  },
  "main": "./dist/core.cjs.js",
  "module": "./dist/core.es.js",
  "typings": "./dist/types/Core/index.d.ts",
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "files": [
    "dist",
    "index.js"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/numl-react.git"
  },
  "bugs": {
    "url": "https://github.com/numldesign/numl-react/issues"
  },
  "gitHead": "3f2d9bc03b831b7d51e0fe93290501a7ba44153a",
  "dependencies": {
    "@numl-react/atoms": "^0.0.14",
    "@numl-react/core": "^0.0.15",
    "@numl-react/utility": "^0.0.15"
  }
}
