{
  "name": "@numl-react/media-card",
  "version": "0.0.21",
  "description": "A simple and accessible component for React by numl design",
  "keywords": [
    "design",
    "system",
    "components",
    "react",
    "numl",
    "uikit"
  ],
  "author": "Numl.Design Team",
  "license": "MIT",
  "homepage": "https://github.com/numldesign/numl-react#readme",
  "scripts": {
    "prebuild": "yarn build:types",
    "index": "echo 'export * from \"./dist/core.es.js\";' > index.js ",
    "install": "yarn",
    "build": "vite build",
    "build:types": "tsc --skipLibCheck --esModuleInterop --emitDeclarationOnly --declaration --jsx react --declarationDir dist/types"
  },
  "main": "./dist/core.cjs.js",
  "module": "./dist/core.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/numl-react.git"
  },
  "bugs": {
    "url": "https://github.com/numldesign/numl-react/issues"
  },
  "gitHead": "3f2d9bc03b831b7d51e0fe93290501a7ba44153a",
  "dependencies": {
    "@numl-react/atoms": "^0.0.20",
    "@numl-react/core": "^0.0.20",
    "@numl-react/elements": "^0.0.20",
    "@numl-react/molecules": "^0.0.20",
    "@numl-react/utility": "^0.0.20"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "jest": "26.6.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.2.4"
  }
}
