{
  "name": "@numl-react/core",
  "version": "0.0.22",
  "description": "Core Packages of Numl React by NUML.Design.",
  "keywords": [
    "components",
    "elements",
    "react",
    "numl-react",
    "design-system",
    "react-components",
    "accessible",
    "ui-library",
    "ui-kit"
  ],
  "author": "Numl.Design Team",
  "license": "MIT",
  "homepage": "https://github.com/numldesign/numl-react#readme",
  "scripts": {
    "postbuild": "yarn build:types",
    "build": "vite build",
    "build:types": "npx tsc index.ts --esModuleInterop --skipLibCheck --skipDefaultLibCheck --declaration --emitDeclarationOnly --outDir dist/types --jsx react"
  },
  "main": "./dist/core.cjs.js",
  "module": "./dist/core.es.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "files": [
    "dist",
    "index.js",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/numl-react.git"
  },
  "bugs": {
    "url": "https://github.com/numldesign/numl-react/issues"
  },
  "gitHead": "3f2d9bc03b831b7d51e0fe93290501a7ba44153a",
  "dependencies": {
    "@numl-react/atoms": "0.0.22",
    "@numl-react/elements": "0.0.22",
    "@numl-react/molecules": "0.0.22",
    "@numl-react/utility": "0.0.22",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "jest": "^26.6.3",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.2.4"
  }
}
